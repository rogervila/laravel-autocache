{
    "name": "rogervila/laravel-autocache",
    "description": "Automatic Laravel Eloquent Models Cache",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "rogervila@me.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LaravelAutoCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "rogervila/php-sonarqube-scanner": "^0 || ^1",
        "orchestra/testbench": "^3 || ^4 || ^5 || ^6",
        "swayok/alternative-laravel-cache": "^5.4",
        "cache/filesystem-adapter": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAutoCache\\AutocacheServiceProvider"
            ]
        }
    }
}
